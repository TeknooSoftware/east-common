{
    "name": "teknoo/east-website",
    "description": "East package to run a basic website",
    "license": "MIT",
    "type": "library",
    "keywords": ["east","website","cms"],
    "homepage": "http://teknoo.software/website",
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\Website\\": "src/universal",
            "Teknoo\\East\\WebsiteBundle\\": "src/symfony",
            "Teknoo\\East\\Website\\Doctrine\\": "src/doctrine",
            "Teknoo\\Tests\\East\\Website\\": "tests/universal",
            "Teknoo\\Tests\\East\\WebsiteBundle\\": "tests/symfony",
            "Teknoo\\Tests\\East\\Website\\Doctrine\\": "tests/doctrine"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "doctrine/common": "~2.11",
        "doctrine/annotations": "~1.8",
        "gedmo/doctrine-extensions": "~2.4",
        "teknoo/immutable": "^2.0.0-beta5",
        "teknoo/states": "^4.0.0-beta5",
        "teknoo/recipe": "^2.0.0-beta4",
        "teknoo/east-foundation": "^2.0.0-beta8",
        "php-di/php-di": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.4",
        "squizlabs/php_codesniffer": "~3.5",
        "pdepend/pdepend": "~2.5",
        "phpmd/phpmd": "~2.7",
        "sebastian/phpcpd": "~4.1",
        "phploc/phploc": "~5.0",
        "phpstan/phpstan": "~0.11",
        "behat/behat": "~3.5",
        "behat/transliterator": "~1.2",
        "symfony/console": "~4.4||~5.0",
        "symfony/yaml": "~4.4||~5.0",
        "symfony/form": "~4.4||~5.0",
        "symfony/templating": "~4.4||~5.0",
        "symfony/security": "~4.4||~5.0",
        "zendframework/zend-diactoros": "~1.8||~2.2",
        "stof/doctrine-extensions-bundle": "~1.3",
        "doctrine/mongodb-odm": "~1.3",
        "doctrine/mongodb-odm-bundle": "~3.6"
    },
    "archive": {
        "exclude": ["features", "tests", "vendor", ".idea"]
    },
    "extra": {
        "thanks": {
            "name": "teknoo/east-foundation",
            "url": "https://github.com/TeknooSoftware/east-foundation"
        }
    }
}
