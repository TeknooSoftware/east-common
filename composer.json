{
    "name": "teknoo/east-website",
    "type": "library",
    "description": "Universal package, following the #East programming philosophy, build on Teknoo/East-Foundation (and Teknoo/Recipe), and implementing a basic CMS to display dynamics pages with different types and templates.",
    "keywords": ["east","website","cms"],
    "homepage": "http://teknoo.software/website",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richarddeloge@gmail.com",
            "role":"Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/east-website/issues",
        "source": "https://github.com/TeknooSoftware/east-website"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\Website\\": "src/",
            "Teknoo\\East\\WebsiteBundle\\": "infrastructures/symfony",
            "Teknoo\\East\\Website\\Doctrine\\": "infrastructures/doctrine",
            "Teknoo\\Tests\\East\\Website\\": "tests/universal",
            "Teknoo\\Tests\\East\\WebsiteBundle\\": "tests/infrastructures/symfony",
            "Teknoo\\Tests\\East\\Website\\Doctrine\\": "tests/infrastructures/doctrine"
        }
    },
    "require": {
        "php": "~8.0",
        "ext-json": "*",
        "php-di/php-di": "^6.3.5",
        "teknoo/immutable": "^2.0.9",
        "teknoo/states": "^5.0.6",
        "teknoo/recipe": "^3.3.0",
        "teknoo/east-foundation": "^5.3.5"
    },
    "require-dev": {
        "ext-simplexml" : "*",
        "ext-mongodb" : "*",
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^9.5.9",
        "squizlabs/php_codesniffer": "~3.6",
        "pdepend/pdepend": "~2.10",
        "sebastian/phpcpd": "^6.0.3",
        "phploc/phploc": "^7.0.2",
        "phpstan/phpstan": "^0.12.99",
        "behat/behat": "~3.8",
        "behat/transliterator": "~1.3",
        "teknoo/bridge-phpdi-symfony": "^5.0.4",
        "psr/http-message":"^1.0.1",
        "psr/http-factory":"^1.0.1",
        "symfony/config": "^5.3.4",
        "symfony/console": "^5.3.7",
        "symfony/dependency-injection": "^5.3.7",
        "symfony/form": "^5.3.7",
        "symfony/http-kernel": "^5.3.7",
        "symfony/options-resolver": "^5.3.7",
        "symfony/password-hasher": "^5.3.7",
        "symfony/psr-http-message-bridge": "~2.1",
        "symfony/security-core": "^5.3.7",
        "symfony/security-http": "^5.3.7",
        "symfony/security-bundle": "^5.3.7",
        "symfony/templating": "^5.3.7",
        "symfony/translation": "^5.3.7",
        "symfony/yaml": "^5.3.6",
        "twig/twig": "~3.3",
        "laminas/laminas-diactoros": "~2.6",
        "doctrine/common": "^3.1.2",
        "doctrine/persistence": "^2.2.2",
        "doctrine/mongodb-odm": "^2.2.2",
        "doctrine/mongodb-odm-bundle": "^4.3.0",
        "league/oauth2-client": "~2.6",
        "knpuniversity/oauth2-client-bundle": "~2.8"
    },
    "archive": {
        "exclude": ["features", "tests", "vendor", ".idea"]
    },
    "extra": {
        "thanks": {
            "name": "teknoo/east-foundation",
            "url": "https://github.com/TeknooSoftware/east-foundation"
        }
    }
}
