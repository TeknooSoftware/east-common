{
    "name": "teknoo/east-common",
    "type": "library",
    "description": "Universal package, following the #East programming philosophy, build on Teknoo/East-Foundation (and Teknoo/Recipe), providing components (user management, object persistence, template rendering, ..) for the creation of web application or website.",
    "keywords": ["east","common","application"],
    "homepage": "https://teknoo.software/east-collection/common",
    "license": ["BSD-3-Clause"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richard@teknoo.software",
            "role": "Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/east-common/issues",
        "source": "https://github.com/TeknooSoftware/east-common"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "config": {
        "audit": {
            "abandoned": "report"
        },
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\Common\\": "src/",
            "Teknoo\\East\\CommonBundle\\": "infrastructures/symfony",
            "Teknoo\\East\\Common\\Doctrine\\": "infrastructures/doctrine",
            "Teknoo\\East\\Common\\Flysystem\\": "infrastructures/flysystem",
            "Teknoo\\East\\Common\\Minify\\": "infrastructures/minify",
            "Teknoo\\Tests\\East\\Common\\": "tests/universal",
            "Teknoo\\Tests\\East\\CommonBundle\\": "tests/infrastructures/symfony",
            "Teknoo\\Tests\\East\\Common\\Doctrine\\": "tests/infrastructures/doctrine",
            "Teknoo\\Tests\\East\\Common\\Flysystem\\": "tests/infrastructures/flysystem",
            "Teknoo\\Tests\\East\\Common\\Minify\\": "tests/infrastructures/minify"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Tests\\East\\Common\\Behat\\": "tests/behat",
            "Teknoo\\Tests\\East\\Common\\Support\\PHPStan\\": "tests/support/PHPStan"
        }
    },
    "require": {
        "php": "^8.4",
        "php-di/php-di": "^7.0.11",
        "teknoo/immutable": "^3.0.21",
        "teknoo/states": "^7",
        "teknoo/recipe": "^7",
        "teknoo/east-foundation": "^9"
    },
    "require-dev": {
        "ext-mongodb" : "*",
        "ext-tidy" : "*",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^12.3",
        "squizlabs/php_codesniffer": "^3.13.2",
        "phpstan/phpstan": "^2.1.22",
        "phpstan/extension-installer": "^1.4.3",
        "symfony/property-access": "^6.4.24||^7.3",
        "behat/behat": "^3.23",
        "teknoo/bridge-phpdi-symfony": "^7",
        "doctrine/common": "^3.5",
        "doctrine/mongodb-odm": "^2.11.2",
        "doctrine/mongodb-odm-bundle": "^5.3.1",
        "doctrine/persistence": "^4",
        "guzzlehttp/promises": "^2.2",
        "knpuniversity/oauth2-client-bundle": "^2.18.3",
        "laminas/laminas-diactoros": "^3.6",
        "league/flysystem": "^3.30",
        "league/flysystem-local": "^3.30",
        "league/mime-type-detection": "1.16",
        "league/oauth2-client": "^2.8.1",
        "matthiasmullie/minify": "^1.3.75",
        "scheb/2fa-backup-code": "^7.11",
        "scheb/2fa-bundle": "^7.11",
        "scheb/2fa-google-authenticator": "^7.11",
        "scheb/2fa-totp": "^7.11",
        "spomky-labs/otphp": "^11.3",
        "symfony/cache": "^6.4.24||^7.3",
        "symfony/cache-contracts": "^3.6",
        "symfony/clock": "^6.4.24||^7.3",
        "symfony/config": "^6.4.24||^7.3",
        "symfony/console": "^6.4.24||^7.3",
        "symfony/dependency-injection": "^6.4.24||^7.3",
        "symfony/error-handler": "^6.4.24||^7.3",
        "symfony/expression-language": "^6.4.24||^7.3",
        "symfony/event-dispatcher": "^6.4.24||^7.3",
        "symfony/framework-bundle": "^6.4.24||^7.3",
        "symfony/filesystem": "^6.4.24||^7.3",
        "symfony/form": "^6.4.24||^7.3",
        "symfony/http-kernel": "^6.4.24||^7.3",
        "symfony/mailer": "^6.4.24||^7.3",
        "symfony/mime": "^6.4.24||^7.3",
        "symfony/notifier": "^6.4.24||^7.3",
        "symfony/password-hasher": "^6.4.24||^7.3",
        "symfony/property-info": "^6.4.24||^7.3",
        "symfony/psr-http-message-bridge": "^6.4.24||^7.3",
        "symfony/routing": "^6.4.24||^7.3",
        "symfony/security-bundle": "^6.4.24||^7.3",
        "symfony/security-core": "^6.4.24||^7.3",
        "symfony/security-http": "^7.3",
        "symfony/string": "^6.4.24||^7.3",
        "symfony/translation-contracts": "^3.6",
        "symfony/twig-bridge": "^6.4.24||^7.3",
        "symfony/validator": "^6.4.24||^7.3",
        "symfony/var-dumper": "^6.4.24||^7.3",
        "symfony/yaml": "^6.4.24||^7.3",
        "twig/twig": "^3.21.1",
        "twig/cssinliner-extra": "^3.21",
        "twig/inky-extra": "^3.21",
        "rector/rector": "*"
    },
    "archive": {
        "exclude": ["features", "tests", "vendor", ".idea"]
    },
    "extra": {
        "thanks": {
            "name": "teknoo/east-foundation",
            "url": "https://github.com/TeknooSoftware/east-foundation"
        }
    }
}
