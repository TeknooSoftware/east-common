{
    "name": "teknoo/east-common",
    "type": "library",
    "description": "Universal package, following the #East programming philosophy, build on Teknoo/East-Foundation (and Teknoo/Recipe), providing components (user management, object persistence, template rendering, ..) for the creation of web application or website.",
    "keywords": ["east","common","application"],
    "homepage": "https://teknoo.software/east-collection/common",
    "license": ["BSD-3-Clause"],
    "authors": [
        {
            "name": "Richard DÃ©loge",
            "email": "richard@teknoo.software",
            "role": "Software architect, Lead developer"
        }
    ],
    "support": {
        "issues": "https://github.com/TeknooSoftware/east-common/issues",
        "source": "https://github.com/TeknooSoftware/east-common"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/teknoo_software"
        },
        {
            "type": "github",
            "url": "https://github.com/sponsors/TeknooSoftware"
        }
    ],
    "config": {
        "audit": {
            "abandoned": "report"
        },
        "optimize-autoloader": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Teknoo\\East\\Common\\": "src/",
            "Teknoo\\East\\CommonBundle\\": "infrastructures/symfony",
            "Teknoo\\East\\Common\\Doctrine\\": "infrastructures/doctrine",
            "Teknoo\\East\\Common\\Flysystem\\": "infrastructures/flysystem",
            "Teknoo\\East\\Common\\Minify\\": "infrastructures/minify",
            "Teknoo\\Tests\\East\\Common\\": "tests/universal",
            "Teknoo\\Tests\\East\\CommonBundle\\": "tests/infrastructures/symfony",
            "Teknoo\\Tests\\East\\Common\\Doctrine\\": "tests/infrastructures/doctrine",
            "Teknoo\\Tests\\East\\Common\\Flysystem\\": "tests/infrastructures/flysystem",
            "Teknoo\\Tests\\East\\Common\\Minify\\": "tests/infrastructures/minify"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Teknoo\\Tests\\East\\Common\\Behat\\": "tests/behat",
            "Teknoo\\Tests\\East\\Common\\Support\\PHPStan\\": "tests/support/PHPStan"
        }
    },
    "require": {
        "php": "^8.4",
        "php-di/php-di": "^7.1.1",
        "teknoo/immutable": "^3.0.22",
        "teknoo/states": "^7.1.2",
        "teknoo/recipe": "^7.2.2",
        "teknoo/east-foundation": "^9.0.4"
    },
    "require-dev": {
        "ext-mongodb" : "*",
        "ext-tidy" : "*",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^12.5.3",
        "squizlabs/php_codesniffer": "^4.0.1",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/extension-installer": "^1.4.3",
        "symfony/property-access": "^6.4.24||^7.3||^8",
        "behat/behat": "^3.27",
        "teknoo/bridge-phpdi-symfony": "^7.1.2",
        "doctrine/common": "^3.5",
        "doctrine/mongodb-odm": "^2.15.1",
        "doctrine/mongodb-odm-bundle": "^5.5.1",
        "doctrine/persistence": "^4.1.1",
        "guzzlehttp/promises": "^2.3",
        "knpuniversity/oauth2-client-bundle": "^2.20.1",
        "laminas/laminas-diactoros": "^3.8",
        "league/flysystem": "^3.30.2",
        "league/flysystem-local": "^3.30.2",
        "league/mime-type-detection": "1.16",
        "league/oauth2-client": "^2.9",
        "matthiasmullie/minify": "^1.3.75",
        "scheb/2fa-backup-code": "^8",
        "scheb/2fa-bundle": "^8",
        "scheb/2fa-google-authenticator": "^8",
        "scheb/2fa-totp": "^8",
        "spomky-labs/otphp": "^11.3",
        "symfony/cache": "^6.4.24||^7.3||^8",
        "symfony/cache-contracts": "^3.6",
        "symfony/clock": "^6.4.24||^7.3||^8",
        "symfony/config": "^6.4.24||^7.3||^8",
        "symfony/console": "^6.4.24||^7.3||^8",
        "symfony/dependency-injection": "^6.4.24||^7.3||^8",
        "symfony/error-handler": "^6.4.24||^7.3||^8",
        "symfony/expression-language": "^6.4.24||^7.3||^8",
        "symfony/event-dispatcher": "^6.4.24||^7.3||^8",
        "symfony/framework-bundle": "^6.4.24||^7.3||^8",
        "symfony/filesystem": "^6.4.24||^7.3||^8",
        "symfony/form": "^6.4.24||^7.3||^8",
        "symfony/http-kernel": "^6.4.24||^7.3||^8",
        "symfony/mailer": "^6.4.24||^7.3||^8",
        "symfony/mime": "^6.4.24||^7.3||^8",
        "symfony/notifier": "^6.4.24||^7.3||^8",
        "symfony/password-hasher": "^6.4.24||^7.3||^8",
        "symfony/property-info": "^6.4.24||^7.3||^8",
        "symfony/psr-http-message-bridge": "^6.4.24||^7.3||^8",
        "symfony/routing": "^6.4.24||^7.3||^8",
        "symfony/security-bundle": "^6.4.24||^7.3||^8",
        "symfony/security-core": "^6.4.24||^7.3||^8",
        "symfony/security-http": "^6.4.24||^7.3||^8",
        "symfony/string": "^6.4.24||^7.3||^8",
        "symfony/translation-contracts": "^3.6.1",
        "symfony/twig-bridge": "^6.4.24||^7.3||^8",
        "symfony/validator": "^6.4.24||^7.3||^8",
        "symfony/var-dumper": "^6.4.24||^7.3||^8",
        "symfony/yaml": "^6.4.24||^7.3||^8",
        "twig/twig": "^3.22.1",
        "twig/cssinliner-extra": "^3.22",
        "twig/inky-extra": "^3.22"
    },
    "archive": {
        "exclude": ["features", "tests", "vendor", ".idea"]
    },
    "extra": {
        "thanks": {
            "name": "teknoo/east-foundation",
            "url": "https://github.com/TeknooSoftware/east-foundation"
        }
    }
}
