parameters:
    scopeClass: Teknoo\States\PHPStan\Analyser\Scope
    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false
    ignoreErrors:
        -
            message: '#Parameter \#1 \$translatableSetter of class [a-zA-Z0-9\\_]+ constructor expects \(callable\(\): mixed\)\|null#'
            path: infrastructures/doctrine/di.php
        -
            message: '#Method [a-zA-Z0-9\\_]+::getCurrentDateTime\(\) should return DateTimeInterface but returns DateTimeInterface\|null#'
            path: infrastructures/symfony/AdminEndPoint/AdminEditEndPoint.php
        -
            message: '#Call to an undefined method [a-zA-Z0-9\\_]+&[a-zA-Z0-9\\_]+::setPublishedAt\(\)#'
            path: infrastructures/symfony/AdminEndPoint/AdminEditEndPoint.php
        -
            message: '#Cannot cast array<string>\|string\|null to string.#'
            path: infrastructures/symfony/Command/CreateUserCommand.php
        -
            message: '#Parameter \#1 \$argument of class ReflectionClass constructor expects class-string<T of object>\|T of object, string given.#'
            path: infrastructures/symfony/Provider/UserProvider.php
        - '#MongoGridFS#'
services:
	-
		class: Teknoo\States\PHPStan\MethodsClassExtension
		tags:
			- phpstan.broker.methodsClassReflectionExtension